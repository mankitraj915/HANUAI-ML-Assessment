ync function GetCustomerID() {\n  function buf2hex(buffer) { // buffer is an ArrayBuffer\n    return [...new Uint8Array(buffer)]\n      .map(x => x.toString(16).padStart(2, '0'))\n      .join('');\n  }\n  \n  // Check for performance consent for setting user_id is done in the Data Element itself.\n  // Removing the check for performance consent here so that hashedAccountId can be set regardless of consent as it is used for creating PPID as well.\n  const cookieObj = Object.fromEntries(window.document.cookie.split(';').map(x => x.trim().split('=')))\n  //If the ta object exists, then the user is somewhat signed in, so return the cid cookie with is the customer id\n  //cid will exist if the user has logged in in the past and we can use that cookie for our hashedAccountId\n  const possibleCid = cookieObj.ta || cookieObj.cid ? cookieObj.cid : null;\n  const possibleAid = cookieObj.ta || cookieObj.aid ? cookieObj.aid : null;\n\n  if(possibleAid) // aid is the hashed account id from the server if it's returned\n  {\n    localStorage.setItem('hashedAccountID', possibleAid)\n  }\n  else if(possibleCid) // if aid is not returned, then we just hash ourselves (as a temp measure until server hashing is done)\n  {\n    const cid = JSON.parse(decodeURIComponent(possibleCid));\n    const customerID = cid.id.replace(\"{\", \"\").replace(\"}\", \"\")\n    const hashedAccountID = buf2hex( await crypto.subtle.digest('SHA-256', new TextEncoder().encode(customerID)))\n    localStorage.setItem('hashedAccountID', hashedAccountID)\n  }\n  else // there's no user, set empty string\n  {\n    localStorage.setItem('hashedAccountID', \"\")\n  }\n}\n\nGetCustomerID();",language:"javascript"}}]},{id:"RLa43af9e2720e4bf49887d21e0d047001",name:"3rd_Party/Cart_PageLoad [GFL] | OT",events:[{modulePath:"data-layer-manager-search-discovery/src/lib/events/data_layer_push_event.js",settings:{eventType:"cart/ANALYTICS_CART_LOADED",subscription:null},ruleOrder:55}],conditions:[{modulePath:"core/src/lib/cond