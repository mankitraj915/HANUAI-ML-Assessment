source:"// \ud83d\udea9 Probably a better way to implement this in 2024.\n\nvar getEnhancements = function(offers) {\n  \n  var finalOffers = [];\n  var reducedJSONOffers = offers.filter(function(o) {\n   return o.type === \"json\";\n  });\n  \n  var reducedHTMLOffers = offers.filter(function(o) {\n   return o.type === \"html\";\n  });\n  \n  if (reducedJSONOffers.length > 0) {\n    reducedJSONOffers.forEach(function(offer){\n      finalOffers.push(offer.content);\n    })\n    //finalOffers = finalOffers.concat(reducedJSONOffers[0].content)\n  }\n  \n  if (reducedHTMLOffers.length > 0) {\n      reducedHTMLOffers.forEach(function(offer, index){\n          try {\n            finalOffers.push(JSON.parse(offer.content));\n          }\n          catch(e){\n            _satellite.logger.warn('==== Target: GETOFFER FAILURE: The reco at index: ' + index + ' failed to parse to JSON and  will not be in enhancements event ====');\n          }\n      });\n  }\n  return finalOffers.length > 0 ? finalOffers : null;\n};\n\n//NOTE: if this is necessary needs to be reevaluated, was this just for mobile apps?\n// Jan 16th: we fixed issue with corrupted mboxSession being empty param,\n// (fixed by monkey patching req url with value)\n// however, we still seeing issue with bad req, so logging entire req\n// to see if there are other issues\n(function() {\n    var ajaxOpen = XMLHttpRequest.prototype.open;\n    XMLHttpRequest.prototype.open = function(method, reqUrl) {\n      if (!/bestbuycanada\\.tt\\.omtrdc\\.net/.test(reqUrl)) {\n        // return early\n        return ajaxOpen.apply(this, [method, reqUrl]);\n      }\n\n      // patch fix\n      var DEFAULT_MBOX_SESSION = 'e872a74cc53145aea95c5f240cadc1e5';\n      var patchedUrl = reqUrl;\n      if (/mboxSession=\\&/i.test(reqUrl)) {\n        var previousSession = _satellite.cookie.get('dtm_mSession') || DEFAULT_MBOX_SESSION;\n        var patchedMboxSession = 'mboxSession=' + previousSession + '&';\n        patchedUrl = reqUrl.replace(/mboxSession=\\&/ig, patchedMboxSession);\n      } else {\n        var mboxSessionStr = _satellite.cookie.get('mbox').split('|').filter(function(el) { return /session/ig.test(el); }).join() || '';\n        mboxSessionStr = mboxSessionStr.split('#')[1] || DEFAULT_MBOX_SESSION;\n        _satellite.cookie.set('dtm_mSession', mboxSessionStr, {expires: 7});\n      }\n\n      return ajaxOpen.apply(this, [method, patchedUrl]);\n    };\n})();\nwindow.mboxFetch = function(mbox, params, shouldApplyOffers, payload ) {\n  try {\n    var mboxCookieStr = _satellite.cookie.get('mbox') || '';\n    var AMCVCookie = _satellite.cookie.get('AMCV_D6E638125859683E0A495D2D@AdobeOrg') || '';\n    var paramsObj = Object.assign({}, params);\n    paramsObj.at_property = \"fc53a67f-eeaf-6c6d-9c82-64ad36aae2bd\";\n    window.persistentMetaData = window.persistentMetaData || {};\n    window.persistentMetaData.mboxCookie = mboxCookieStr + ',' + AMCVCookie;\n    window.persistentMetaData.mboxLoadTime = window.persistentMetaData.mboxLoadTime || {};\n    if(payload && payload.request) {\n      payload.request.property = {token: paramsObj.at_property}\n    }\n\n    _satellite.logger.log('=== Target: Fetching mbox ' + mbox +  ' ===');\n    adobe.target.getOffers(payload || {\n        request: {\n            execute: {\n                pageLoad: {\n                    parameters: params\n                }\n            },\n            property: {\n                token: paramsObj.at_property\n            }\n        }\n    }).then(function(offers) {\n      window.__lastOffers = offers;\n      if(offers && offers.execute && offers.execute.pageLoad && offers.execute.pageLoad.options) {\n         _satellite.logger.log(\"==== Target: Offers found ====\");\n          if(shouldApplyOffers) {\n            var isProductPurchasable = true;\n            try {\n              if(AppEventData.computedState && AppEventData.computedState.state) {\n                var currentState = AppEventData.computedState.state;\n                if (currentState.product && currentState.product.availability) {\n                  //isProductPurchasable = currentState.product.availability.pickup.purchasable || currentState.product.availability.shipping.purchasable;\n                  isProductPurchasable = currentState.product.availability.shipping.purchasable;\n                }\n              }\n            } catch(e) {\n              console.error('error in detecting product availability', e)\n            }\n              _satellite.logger.log(\"==== Target: Applying offers ====\");\n                // Notify Enhancements module\n                var options = [];\n                \n                // This is NOT a boolean, it's a reference to the first set of boughtAlsoBought items\n                // It's also the variable where we'll store all our boughtAlsoBought items that we eventually want to show to the customer\n                var firstBoughtAlsoBought;\n                offers.execute.pageLoad.options.forEach(function(option) {\n                  \n                  // Parsed data for the current option, if it contains boughtAlsoBought data. \n                  var parsedContent;\n                  var isUnavailableRecommendation = false;\n                  if(typeof(option.content) === \"string\" && option.content.indexOf(\"product.recommendations.unavailable\") !== -1 && !isProductPurchasable) {\n                    try {\n                      option.content = option.content.split(\"product.recommendations.unavailable\").join(\"product.recommendations.boughtAlsoBought\")\n                      parsedContent = JSON.parse(option.content);\n                      isUnavailableRecommendation = true;\n                    } catch (e) {\n                      console.error('Error parsing unavailable recommendation', e);\n                      console.log('Error', option);\n                    }\n                  } else if(typeof(option.content) === \"string\" && option.content.indexOf(\"product.recommendations.boughtAlsoBought\") !== -1) {\n                    option.content = option.content.split(\"\\\\ \").join(\"\");\n                    try {\n                      parsedContent = JSON.parse(option.content);\n                    } catch (e) {\n                      console.error('Error parsing recommendation', e);\n                      console.log('Error', option);\n                    }\n                  } else {\n                    options.push(option);\n                  }\n                  if(!firstBoughtAlsoBought && parsedContent && parsedContent.products && parsedContent.products.length) {\n                    firstBoughtAlsoBought = option;\n                \n                  // If we already have a boughtAlsoBought, then \n                  } else if(firstBoughtAlsoBought) {\n                    \n                    // Parse out the boughtAlsoBought data collection\n                    var parsedFirstBoughtAlsoBought = JSON.parse(firstBoughtAlsoBought.content);\n                    \n                    // If we don't yet have 6 boughtAlsoBought items, then see if we can add some more\n                    if(parsedFirstBoughtAlsoBought.products.length < 6 && parsedContent && parsedContent.products && parsedContent.products.length) {\n                      if(!isUnavailableRecommendation) {\n                        parsedContent.products = parsedContent.products.reverse();\n                      }\n                      \n                      // Go through the current option's boughtAlsoBought items\n                      parsedContent.products.forEach(function(item){\n                        // if current product's available for purchase, then put these new items at the end of the list of boughtAlsoBoughts items\n                        // Otherwise put them at the beginning of the list of boughtAlsoBoughts items\n                        if(!isUnavailableRecommendation) {\n                          parsedFirstBoughtAlsoBought.products.push(item);\n                        } else if(isUnavailableRecommendation) {\n                          parsedFirstBoughtAlsoBought.products.unshift(item);\n                        }\n                      });\n                      var productsWithNoDuplicate = [];\n                      var productsHash = {}\n                      \n                      // De-dupe boughtAlsoBought items gathered so far. Should have no more than 6 items in the final collection.\n                      parsedFirstBoughtAlsoBought.products.forEach(function(newItem){\n                        if(productsWithNoDuplicate.length < 6 && !productsHash[newItem.sku]){\n                          productsHash[newItem.sku] = true;\n                          productsWithNoDuplicate.push(newItem);\n                        }\n                      });\n                      parsedFirstBoughtAlsoBought.products = productsWithNoDuplicate;\n                      firstBoughtAlsoBought.content = JSON.stringify(parsedFirstBoughtAlsoBought);\n                    }\n                  }\n                });\n            \n                if(firstBoughtAlsoBought) {\n                  var items = document.querySelectorAll('div[class^=\"row_\"] div[class^=\"col-xs-6\"][class*=\"col-sm-3\"]');\n                  if(items.length === 0) {\n                    options.push(firstBoughtAlsoBought);\n                  }\n                }\n\n            \n                document.dispatchEvent(window.enhancementsLoadedEvent = new CustomEvent(\n                    \"enhancements-loaded\", {\n                        detail: {\n                            enhancements: getEnhancements(options),\n                        }\n                    }));\n                /*making sure that this event gets processed last by the app*/\n                document.dispatchEvent(new CustomEvent(\n                    \"enhancements-loaded\", {\n                        detail: {\n                            enhancements: [{\n  